    "node-polyglot": "^2.2.2",
    "normalizr": "^3.2.4",
    "numeral": "^2.0.6",
    "open-sans-fontface": "^1.4.0",
    "prop-types": "^15.5.10",
    "qrious": "^4.0.2",
    "query-string": "^5.0.0",
    "react": "^15.6.1",
