{
    "name": "ledger-vault-front",
    "version": "0.1.0",
    "private": true,
    "proxy": "https://ledger-vault-preprod.herokuapp.com",
    "dependencies": {
        "axios": "^0.16.2",
        "bourbon-neat": "^2.1.0",
        "d3": "^4.11.0",
        "email-validator": "^1.1.1",
        "enzyme": "^3.1.0",
        "history": "^4.7.2",
        "ledgerco": "git+https://github.com/LedgerHQ/ledger-node-js-api.git",
        "lodash": "^4.17.4",
        "material-design-icons": "^3.0.1",
        "material-ui": "^0.19.4",
        "moment": "^2.19.1",
        "moxios": "^0.4.0",
        "node-polyglot": "^2.2.2",
        "normalizr": "^3.2.4",
        "open-sans-fontface": "^1.4.0",
        "prop-types": "^15.5.10",
        "qrious": "^4.0.2",
        "query-string": "^5.0.0",
        "react": "^16.0.0",
        "react-contenteditable": "^2.0.4",
        "react-dom": "^16.0.0",
        "react-dropzone": "^4.2.1",
        "react-redux": "^5.0.5",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.1.2",
        "react-router-redux": "^5.0.0-alpha.8",
        "react-tabs": "^2.1.0",
        "react-tap-event-plugin": "^3.0.2",
        "react-tooltip": "^3.4.0",
        "rect-crop": "0.0.0",
        "redux": "^3.7.2",
        "redux-devtools-extension": "^2.13.2",
        "redux-thunk": "^2.2.0",
        "u2f-api": "0.0.9",
        "u2f-api-polyfill": "^0.4.3"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-eslint": "^8.0.1",
        "babel-preset-flow": "^6.23.0",
        "eslint": "^4.10.0",
        "eslint-config-prettier": "^2.6.0",
        "eslint-plugin-flowtype": "^2.39.1",
        "eslint-plugin-react": "^7.4.0",
        "flow-bin": "^0.59.0",
        "flow-typed": "^2.2.1",
        "node-sass-chokidar": "0.0.3",
        "npm-run-all": "^4.0.2",
        "prettier": "^1.7.4",
        "react-scripts": "1.0.17",
        "react-test-renderer": "^16.0.0"
    },
    "scripts": {
        "build-css":
            "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
        "watch-css":
            "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
        "start-js": "react-scripts start",
        "start": "HTTPS=true npm-run-all -p watch-css start-js",
        "build": "npm run build-css && react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "coverage": "react-scripts test --env=jsdom --coverage",
        "eject": "react-scripts eject",
        "lint": "eslint src",
        "prettier": "prettier --write 'src/**/*.js'",
        "postinstall": "flow-typed install -s",
        "flow": "flow"
    }
}
